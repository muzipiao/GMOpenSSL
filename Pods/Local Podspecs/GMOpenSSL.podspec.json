{
  "name": "GMOpenSSL",
  "version": "2.2.6",
  "summary": "编译 OpenSSL 为 framework，版本对应 OpenSSL 1.1.1l。",
  "description": "通过 cocoapods 集成 OpenSSL，编译为 framework，方便使用。",
  "homepage": "https://github.com/muzipiao/GMOpenSSL",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lifei": "lifei_zdjl@126.com"
  },
  "source": {
    "git": "https://github.com/muzipiao/GMOpenSSL.git",
    "tag": "2.2.6"
  },
  "platforms": {
    "ios": "9.0"
  },
  "preserve_paths": "GMFrameworks/openssl.framework",
  "source_files": "GMFrameworks/openssl.framework/Headers/**/*{.h}",
  "public_header_files": "GMFrameworks/openssl.framework/Headers/*.{h}",
  "frameworks": "Foundation",
  "vendored_frameworks": "GMFrameworks/openssl.framework",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
